<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- Define the default state of the button -->
    <!-- Shape, edge, outline (thickness, color), background (graduation, start-end-angle) -->
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="20dip" />
            <stroke android:width="1dip" android:color="#FCFCFC" />
            <gradient android:angle="-90" android:startColor="#EAEAEA" android:endColor="#EAEAEA" />
        </shape>
    </item>
</selector>
